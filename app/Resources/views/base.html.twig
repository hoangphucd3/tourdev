<!DOCTYPE html>
{#<html lang="{{ app.request.locale }}">#}
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/common/icomoon/css/iconmoon.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/bootstrap.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/bootstrap-theme.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/component-gallery.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/jquery.mobile-menu.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/prettyPhoto.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/bootstrap-datetimepicker.min.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/jquery-datetimepicker.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/select2.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/chosen.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/style.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/cs-traveladvisor-plugin.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/default-element.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/responsive.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/cs-woocommerce.css')) }}">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/frontend/css/custom_style.css')) }}">
    {% endblock %}

    {% block core_js %}
        <script src="{{ asset('assets/frontend/scripts/jquery.js') }}"></script>
        <script src="{{ asset('assets/frontend/scripts/bootstrap.min.js') }}"></script>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body class="{% block body_class %}wp-traveladvisor{% endblock %}">
<div class="wrapper full_width">
    <header id="header">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6">
                    <div class="cs-logo">
                        <div class="cs-media">
                            <figure>
                                <a href="{{ absolute_url(path('tour_index')) }}">
                                    <img src="{{ asset('assets/frontend/images/logo.png') }}">
                                </a>
                            </figure>
                        </div>
                    </div>
                </div>

                <div class="col-lg-9 col-md-9 col-sm-6 col-xs-6">
                    <div class="cs-main-nav pull-right">
                        <nav class="main-navigation">
                            <ul class="menu-header-menu">
                                <li>
                                    <a href="{{ path('tour_listing') }}">Danh sách tour</a>
                                </li>
                                {% if not is_granted('IS_AUTHENTICATED_FULLY') %}
                                    <li>
                                        <a href="{{ path('sonata_user_security_login') }}">Đăng nhập</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('sonata_user_registration_register') }}">Đăng ký</a>
                                    </li>
                                {% else %}
                                    <li>
                                        <a href="{{ path('sonata_user_profile_show') }}">Hồ sơ của bạn</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('sonata_user_security_logout') }}">Đăng xuất</a>
                                    </li>
                                {% endif %}
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </header>

    {% block cs_sub_header %}
        {#<div class="cs-sub-header"#}
        {#style="background:url({{ asset('assets/frontend/images/sl_161114_slide-phu-quoc-Hon-Gam-Gi-Dam-Ngang.jpg') }}) no-repeat;background-size:cover;padding-top:200px!important;height:415px!important">#}
        {#</div>#}
    {% endblock %}

    {% block sub_header %}
        <div class="single-trip">
            <ul class="cs-list-detail-slider">
                {% block sub_header_slide %}
                    <img src="{{ absolute_url(asset('assets/frontend/images/Slide-01.jpg')) }}"/>
                    <img src="{{ absolute_url(asset('assets/frontend/images/Slide-03.jpg')) }}"/>
                {% endblock %}
            </ul>
        </div>

        <div class="cs-fixed-sub-header">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="cs-page-title">
                            {% block sub_header_content %}
                                {#<h1>Trang web đặt tour du lịch</h1>#}
                            {% endblock %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {% endblock %}
    <!-- Header End -->

    {% block body %}{% endblock %}
</div>
